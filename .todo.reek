---
DuplicateMethodCall:
  exclude:
  - Tocer::Builder#transform
  - Tocer::Builder#url_suffix
  - Tocer::CLI#build_configuration
TooManyInstanceVariables:
  exclude:
  - Tocer::Builder
  - Tocer::Writer
UtilityFunction:
  exclude:
  - Tocer::Builder#acquire_transfomer
  - Tocer::Elements::CommentBlock#index
TooManyStatements:
  exclude:
  - initialize
  - Tocer::CLI#generate
